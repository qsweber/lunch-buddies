{
    "production": {
        "app_function": "lunch_buddies.app.app",
        "aws_region": "us-west-2",
        "project_name": "lunch-buddies",
        "runtime": "python3.6",
        "s3_bucket": "zappa-vt9hd6vy9",
        "events": [
            {
                "function": "lunch_buddies.app.create_poll_from_queue",
                "expression": "cron(1-59/5 * ? * * *)"
            },
            {
                "function": "lunch_buddies.app.poll_users_from_queue",
                "expression": "cron(2-59/5 * ? * * *)"
            },
            {
                "function": "lunch_buddies.app.close_poll_from_queue",
                "expression": "cron(3-59/5 * ? * * *)"
            },
            {
                "function": "lunch_buddies.app.notify_groups_from_queue",
                "expression": "cron(4-59/5 * ? * * *)"
            }
        ]
    }
}